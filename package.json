{
  "name": "test",
  "displayName": "testextension",
  "description": "Lets see how difficult this is",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:json"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "test.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "Cathystuff.run",
        "title": "RUN (cathy command)",
        "icon": {
          "light": "resources/Bananas.svg",
          "dark": "resources/Bananas.svg"
        }
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "Cathystuff",
          "name": "Catharsys outline"
        }
      ]
    },
    "menus" : {
      "view/item/context": [
        {
          "command": "Cathystuff.run",
          "when": "view == Cathystuff && viewItem == command",
          "group": "inline"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "typescript": "^4.9.4",
    "@vscode/test-electron": "^2.2.2"
  }
}
